@model HospitalProjectStJoeseph.Models.BestWish
@{
    ViewBag.Title = "DeleteConfirm";
    Layout = null;
}

<h3>Confirm Deletion of message from @Model.BestWishSender</h3>

<p>Are you sure you want to delete this message from @Model.BestWishSender?</p>
<form method="post" action="/BestWishes/Delete/@Model.BestWishId">
    <button type="button" onclick="cancel()" class="btn btn-primary">Cancel</button>
    <button type="submit" class="btn btn-danger">Delete</button>
</form>





