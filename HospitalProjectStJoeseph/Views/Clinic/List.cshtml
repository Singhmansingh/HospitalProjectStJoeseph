@model IEnumerable<HospitalProjectStJoeseph.Models.ClinicDto>
@{
    ViewBag.Title = "List";
}

<h2>List</h2>
<a href="/Clinic/New" class="btn btn-success">New Clinic</a>
@foreach (var clinic in Model)
{
    <h2><a href="/Clinic/Details/@clinic.ClinicId">@clinic.ClinicName</a></h2>
    <div><h4>Time: @clinic.ClinicTime</h4></div>
}